<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ViewOrdersClientController">
    <children>
        <Button fx:id="back" layoutX="510.0" layoutY="248.0" mnemonicParsing="false" onAction="#goBack" prefHeight="29.0" prefWidth="61.0" text="Назад" />
        <TableView fx:id="ordersTable" layoutX="19.0" layoutY="22.0" prefHeight="200.0" prefWidth="562.0">
            <columns>
                <TableColumn fx:id="makeColumn" editable="false" prefWidth="87.0" text="Марка" />
                <TableColumn fx:id="modelColumn" editable="false" prefWidth="110.0" text="Модель" />
                <TableColumn fx:id="yearColumn" editable="false" prefWidth="84.0" text="Год" />
                <TableColumn fx:id="colorColumn" editable="false" prefWidth="71.0" text="Цвет" />
                <TableColumn fx:id="countryColumn" editable="false" prefWidth="111.0" text="Сборка" />
                <TableColumn fx:id="insuranceColumn" editable="false" prefWidth="81.0" text="Страховка" />
                <TableColumn fx:id="priceColumn" editable="false" prefWidth="67.0" text="Цена" />
                <TableColumn fx:id="acceptedColumn" editable="false" prefWidth="76.0" text="Принят" />
                <TableColumn fx:id="completedColumn" editable="false" minWidth="0.0" prefWidth="75.0" text="Завершён" />
            </columns>
        </TableView>
        <Button layoutX="254.0" layoutY="238.0" mnemonicParsing="false" onAction="#saveToFile" prefHeight="42.0" prefWidth="87.0" text="Сохранить в файл" textAlignment="CENTER" wrapText="true" />
    </children>
</Pane>
